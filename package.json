{
  "name": "monday.com-captivate-automations",
  "version": "2.4.0",
  "license": "UNLICENSED",
  "main": "dist/app.js",
  "scripts": {
    "start": "ts-node-dev --watch ./.env ./src/*",
    "debug": "ts-node-dev --inspect --respawn ./src/app.ts",
    "expose": "ngrok http 8302",
    "lint": "eslint 'src/**/*.ts' --ext .ts --config .eslintrc.json",
    "build": "tsc --build tsconfig.json --verbose",
    "production": "node ./dist/app.js",
    "prepare": "husky install",
    "publish": "ssh deploy@laura-malta.bnr.la -t \"docker pull aussidavid/captivate-ingrations && docker service update --image aussidavid/captivate-ingrations:latest captivate\"",
    "test": "jest",
    "test:watch": "jest --watch --collectCoverage",
    "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "dependencies": {
    "@sentry/browser": "^6.7.2",
    "@sentry/node": "^6.7.1",
    "@sentry/tracing": "^6.7.2",
    "async-mutex": "^0.3.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "monday-sdk-js": "^0.1.2",
    "sprintf-js": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/chai": "^4.2.19",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.23",
    "@types/jest-plugin-context": "^2.9.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/monday-sdk-js": "^0.1.1",
    "@types/node": "^15.0.0",
    "@types/sprintf-js": "^1.1.2",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-jest": "^27.0.5",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.3.4",
    "concurrently": "^6.0.0",
    "cross-port-killer": "^1.2.1",
    "debug": "^4.0.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-typescript-lite": "^12.0.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "husky": "^6.0.0",
    "jest": "^27.0.5",
    "jest-plugin-context": "^2.9.0",
    "mock-require": "^3.0.1",
    "ngrok": "^4.0.0",
    "nodemon": "^2.0.2",
    "nyc": "^15.1.0",
    "prettier": "^2.3.1",
    "sinon": "^11.0.0",
    "standard-version": "^9.3.0",
    "superagent": "^6.1.0",
    "supertest": "^6.1.3",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.3.4"
  }
}
