{
  "name": "monday-integration-quickstart-app-typescript",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "yarn stop && concurrently \"yarn dev-server\" \"yarn expose\"",
    "dev-server": "ts-node-dev --watch ./.env ./src/app.ts",
    "start": "yarn server",
    "server": "ts-node ./src/app.ts",
    "expose": "ngrok http 8302",
    "stop": "kill-port 8302 && kill-port 4049 && kill-port 4040"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "monday-sdk-js": "^0.1.2",
    "querystring": "^0.2.0",
    "ts-node": "^10.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/monday-sdk-js": "^0.1.1",
    "chai": "^4.1.2",
    "concurrently": "^6.0.0",
    "cross-port-killer": "^1.2.1",
    "debug": "^4.0.0",
    "eslint": "^v7.0.0",
    "mocha": "^9.0.0",
    "mock-require": "^3.0.1",
    "ngrok": "^4.0.0",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "sinon": "^11.0.0",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  }
}
